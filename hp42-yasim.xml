       Engines : Bristol 'Jupiter'
               : 490 ch
               : 9 cylinders
               : Displacement 1752.8 (28.7 l)
               : Masse 730 lb (330 kg)
               : Cruise RPM 1600
               : Compression 5:1
                                       Rayon * Rayon * Mass(kg)   1.7 * 1.7 * 330
               moment d'après la doc : ________________________ = _______________ = 317.9
                                                   3                      3
  -->
  <!-- Engine left exterior -->
  <propeller x="5.878" y="3.844" z="-0.948"
             mass="730"
             moment="317.9"
             radius="1.7"
             cruise-speed="57"
             cruise-rpm="1575"
             cruise-power="480"
             cruise-alt="5000">
             <actionpt x="6.422" y="3.844" z="-0.948"/>
             <piston-engine eng-power="490" 
                            eng-rpm="1575"
                            displacement="1752.8"
                            compression="5">
                            <control-input axis="/controls/engines/engine[0]/throttle" control="THROTTLE"/>
                            <control-input axis="/controls/engines/engine[0]/starter" control="STARTER"/>
                            <control-input axis="/controls/engines/engine[0]/magnetos" control="MAGNETOS"/>
                            <control-input axis="/controls/engines/engine[0]/mixture" control="MIXTURE"/>
             </piston-engine>
  </propeller>

  <!-- Engine left interior -->
  <propeller x="5.878" y="1.903" z="2.502"
